<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape>
            <solid android:color="@android:color/black" />
        </shape>
    </item>
    <item>
        <rotate
            android:fromDegrees="66"
            android:pivotX="50%"
            android:pivotY="50%"
            android:toDegrees="0">
            <shape>
                <gradient
                    android:centerColor="@android:color/transparent"
                    android:centerX="0.3"
                    android:centerY="0.5"
                    android:endColor="@android:color/transparent"
                    android:startColor="@android:color/holo_red_dark"
                    android:type="sweep" />
            </shape>
        </rotate>
    </item>
    <item>
        <rotate
            android:fromDegrees="290"
            android:pivotX="30%"
            android:pivotY="40%"
            android:toDegrees="0">
            <shape>
                <gradient
                    android:centerColor="@android:color/transparent"
                    android:centerX="0.8"
                    android:centerY="0.3"
                    android:endColor="@android:color/transparent"
                    android:startColor="@android:color/holo_blue_dark"
                    android:type="sweep" />
            </shape>
        </rotate>
    </item>
    <item>
        <rotate
            android:fromDegrees="18"
            android:pivotX="30%"
            android:pivotY="40%"
            android:toDegrees="0">
            <shape>
                <gradient
                    android:centerColor="@android:color/transparent"
                    android:centerX="0.1"
                    android:centerY="0.5"
                    android:endColor="@android:color/transparent"
                    android:startColor="@android:color/holo_green_dark"
                    android:type="sweep" />
            </shape>
        </rotate>
    </item>
    <item>
        <rotate
            android:fromDegrees="280"
            android:pivotX="30%"
            android:pivotY="50%"
            android:toDegrees="0">
            <shape>
                <gradient
                    android:centerColor="@android:color/transparent"
                    android:centerX="0.6"
                    android:centerY="0.7"
                    android:endColor="@android:color/transparent"
                    android:startColor="@android:color/holo_orange_dark"
                    android:type="sweep" />
            </shape>
        </rotate>
    </item>
    <item>
        <rotate
            android:fromDegrees="0"
            android:pivotX="30%"
            android:pivotY="50%"
            android:toDegrees="0">
            <shape>
                <gradient
                    android:centerColor="@android:color/transparent"
                    android:centerX="0.0"
                    android:centerY="0.0"
                    android:endColor="@android:color/transparent"
                    android:startColor="@android:color/holo_purple"
                    android:type="sweep" />
            </shape>
        </rotate>
    </item>
</layer-list>
